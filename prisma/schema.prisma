generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = process.env.DATABASE_URL
}

model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  password String
}

model Work {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  material String
  addedYear Int
  size String
  img String
}
